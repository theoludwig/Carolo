{
  "name": "@carolo/rules",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "mdbook build && node ./build.js",
    "dev:mdbook": "mdbook watch",
    "dev:paths": "node ./watch.js",
    "dev": "concurrently --names \"mdbook,paths\" \"npm run dev:mdbook\" \"npm run dev:paths\""
  },
  "dependencies": {
    "@carolo/mdbook": "*"
  },
  "devDependencies": {
    "concurrently": "8.0.1"
  }
}
